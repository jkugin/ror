<h1>catalog</h1>

<table>
	<tr>
		<th>
			name
		</th>
		<th>
			age
		</th>
		<th>
			price
		</th>
	</tr>
	<% @prostitutes.each do |p| %>
		<tr>
			<td>
				<%= p.name %>
			</td>
			<td>
				<%= p.age %>
			</td>
			<td>
				<%= p.price %>
			</td>
			<td>
				<%= link_to 'show', prostitute_path(p) %>
			</td>
			<td>
				<%= link_to 'edit', edit_prostitute_path(p) %>
			</td>
			<td>
				<%= link_to 'destroy', prostitute_path(p),
				method: :delete,
				data: {confirm: 'yes?'} %>
			</td>
		</tr>
	<% end %>
	
</table>

<%= link_to 'new', new_prostitute_path %>
