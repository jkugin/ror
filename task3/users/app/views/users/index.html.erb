<table>
    <thead>
        <th> Email</th>
    </thead>
    <tbody>
        <% @users.each do |user| %>
			<tr>
				<td>
					<%= user.email %>
				</td>
				<td>
					<%= link_to 'show', user %>
				</td>
				<td>
					<%= link_to 'edit', edit_user_path(user) %>
				</td>
				<td>
					<%= link_to 'delete', user_path(user), method: :delete %>
				</td>
			</tr>
        <% end %>
    </tbody>
</table>
<%= link_to 'new', new_user_path %>
