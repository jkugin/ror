<h1> catalog </h1>

<table>
	<tr>
		<th>
			name
		</th>
		<th>
			size
		</th>
	</tr>
	<% @bugs.each do |b| %>
		<tr>
			<td>
				<%= b.name %>
			</td>
			<td>
				<%= b.size %>
			</td>
			<td>
				<%= link_to 'show', bug_path(b) %>
			</td>
			<td>
				<%= link_to 'edit', edit_bug_path(b) %>
			</td>
			<td>
				<%= link_to 'destroy', bug_path(b),
				method: :delete,
				data: {confirm: 'yes?'} %>
			</td>
		</tr>
	<% end %>
</table>


<%= link_to 'new', new_bug_path %>
